<div class="row">
    <div class="col-lg-7">
        <h2>Nuevo contrato</h2>
    </div>
    <div class="col-lg-5">
        <div class="actions pull-right">
            <button class="btn btn-default" data-ng-click="go('contratos.listado')">Volver al listado</button>
            <button class="btn btn-primary"
                    data-ng-disabled="!canSave()"
                    data-ng-click="guardarContrato()">Guardar</button>
        </div>
    </div>
</div>
<div data-ng-include="template('contrato_form')"></div>

