<div class="row">
    <div class="col-lg-7">
        <h2>Tablero de mando</h2>
    </div>
</div>

<div class="row">
    <div class="col-lg-4">
        <fieldset>
            <legend>Filtros</legend>
            <div data-sises-form="element">
                <div data-sises-form-select="empresa"
                     data-options="'empresa'"
                     data-options-label="nombre"
                     data-options-key="id"
                     data-label="'Empresa'"></div>

                <div data-sises-form-input="start"
                     data-type="'date'"
                     data-label="'Inicio'"></div>

                <div data-sises-form-input="end"
                     data-type="'date'"
                     data-label="'Final'"></div>

                <div class="row">
                    <div class="col-lg-8 col-lg-offset-4">
                        <button class="btn btn-default" data-ng-click="load()">
                            <i class="glyphicon glyphicon-refresh"></i> Obtener datos
                        </button>
                    </div>
                </div>
            </div>
        </fieldset>
    </div>
    <div class="col-lg-8">
        <div id="chart"></div>
    </div>
</div>