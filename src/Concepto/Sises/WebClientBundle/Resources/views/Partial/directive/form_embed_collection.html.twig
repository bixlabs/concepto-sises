{% verbatim %}
    <div class="embed-collection">
        <div class="row">
            <div class="col-lg-12">
                <div class="sub-actions pull-right">
                    <button class="btn btn-info btn-sm"
                            data-ng-click="showDialog()">
                        <i class="glyphicon glyphicon-plus-sign"></i> {{ formProperties.label }}
                    </button>
                </div>
            </div>
        </div>

        <table class="table table-hover">
            <thead>
            <tr>
                <th data-ng-repeat="header in tableHeaders" data-ng-bind="header"></th>
            </tr>
            </thead>
            <tbody>
            <tr data-ng-repeat="table_element in elements"
                data-ng-click="editElement(table_element)"
                class="pointer">
                <td data-ng-repeat="field in tableHeaders">
                    <span data-ng-bind="resolveValue(table_element, field)" style="padding-left: 13px"></span>
                </td>
                <td width="20%">
                    <button class="btn btn-warning" title="Elmina este beneficio"
                            data-ng-click="removeElement(table_element)">
                        <i class="glyphicon glyphicon-remove-sign"></i>
                    </button>
                </td>
            </tr>
            </tbody>
        </table>

        <div data-sises-modalize="formProperties.handler" data-title="{{formProperties.label}}">
            <div data-sises-form="element" data-errors="errors" >
                <div data-ng-transclude></div>
            </div>
        </div>
    </div>
{% endverbatim %}